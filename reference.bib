journal = {J. Fluid Mech.},
keywords = {faraday,patterns},
pages = {301--330},
primaryClass = {patt-sol},
title = {{Pattern Formation In Weakly Damped Parametric Surface Waves}},
url = {http://journals.cambridge.org/action/displayAbstract?aid=13231{\%}5Cnhttp://www.journals.cambridge.org/abstract{\_}S0022112096004764},
