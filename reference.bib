file = {:C$\backslash$:/Users/fpena/Documents/Mendeley Desktop/2017 - Pe{\~{n}}a-Polo et al. - Faraday wave patterns on a square cell network.pdf:pdf},
issn = {07234864},
journal = {Experiments in Fluids},
number = {5},
pages = {47},
title = {{Faraday wave patterns on a square cell network}},
volume = {58},
year = {2017}
}
