@article{Li2014,
abstract = {Ligament formation from the surface of a horizontal liquid layer subject to a vertical vibration (Faraday instability) is a crucial part of the atomization process because it is the transition phase for droplet generation. Based on the numerical solutions of the two-dimensional incompressible Euler equations for a prototype Faraday instability flow, we explored physically how a liquid ligament that is dynamically free from the vibrating liquid layer and behaves like a jet can be produced. According to linear theory, the suction of liquid from the trough portion to the crest portion creates an amplified crest. The amplified crest is always pulled back to the liquid layer in linear theory, no matter how largely the surface deforms; thus, a dynamically freed ligament never forms. However, under nonlinear conditions produced by large surface deformation, the impinging liquid flow from the trough portion enhances the pressure at the high crest (ligament) root. This pressure enhancement has two major effects. First, it reduces the amount of liquid sucked from the trough portion, which abates the increase in the crest height compared with that associated with linear theory. Second, it forms a local maximum pressure at the crest root; in this case, the ligament above this location becomes dynamically free from the motion of the bottom substrate in the laboratory reference frame. Liquid elements continuously enter the dynamically freed liquid region, producing a slender ligament from the liquid layer. {\textcopyright} 2013 Elsevier Ltd.},
author = {Li, Yikai and Umemura, Akira},
doi = {10.1016/j.ijmultiphaseflow.2013.12.002},
file = {:C$\backslash$:/Users/fpena/Documents/Mendeley Desktop/2014 - Li, Umemura - Two-dimensional numerical investigation on the dynamics of ligament formation by Faraday instability.pdf:pdf},
issn = {03019322},
journal = {International Journal of Multiphase Flow},
keywords = {CLSVOF,Faraday instability,Ligament,Nonlinear effect},
pages = {64--75},
publisher = {Elsevier Ltd},
title = {{Two-dimensional numerical investigation on the dynamics of ligament formation by Faraday instability}},
url = {http://dx.doi.org/10.1016/j.ijmultiphaseflow.2013.12.002},
volume = {60},
year = {2014}
}
@article{Li2014a,
abstract = { A vertically vibrating liquid layer produces liquid ligaments that disintegrate to form a spray with drops of a controllable size. Previous experimental investigations of ultrasonic atomisation have shown that when such a spray forms, there exists a predominant surface-wave mode from which drops are generated with a mean diameter that follows Lang's equation. In this paper, we determined this predominant surface-wave mode physically and, by utilising the coupled level-set and volume-of-fluid method, we numerically studied the threshold condition for spray formation based on a cell model of the predominant surface wavelength that excludes the effects of the container walls. We defined a condition whereby the broken drop holds a zero area-averaged vertical velocity in the laboratory reference frame as the criterion for the formation of a spray. The results of our calculations indicated that the onset of a spray occurs in the subharmonic unstable region for a threshold dimensionless forcing strength    {\$}{\{}\backslashit\backslashbeta{\}}{\_}{\{}c{\}}=({\{}\backslashit\backslashrho{\}}{\_}{\{}l{\}}{\{}\backslashit\backslashDelta{\}}{\_}{\{}0{\}}{\^{}}{\{}3{\}}{\{}\backslashit\backslashOmega{\}}{\^{}}{\{}2{\}})/{\{}\backslashit\backslashsigma{\}}\backslashsim O(1){\$}   , where    {\$}{\{}\backslashit\backslashrho{\}}{\_}{\{}l{\}}{\$}   and    {\$}{\{}\backslashit\backslashsigma{\}}{\$}   denote the liquid density and surface tension coefficient, respectively,    {\$}{\{}\backslashit\backslashDelta{\}}{\_}{\{}0{\}}{\$}   is the forcing displacement amplitude and    {\$}{\{}\backslashit\backslashOmega{\}}{\$}   is the forcing angular frequency. Spray formation due to the Faraday instability can be considered as a process whereby the liquid layer absorbs energy from the inertial force, and releases it by producing drops that leave the surface of the liquid layer. We demonstrated that for a deep liquid layer, the threshold condition for the formation of a spray is determined only by the forcing strength, and is independent of the initial conditions of the liquid surface. },
author = {Li, Yikai and Umemura, Akira},
doi = {10.1017/jfm.2014.569},
file = {:C$\backslash$:/Users/fpena/Documents/Mendeley Desktop/2014 - Li, Umemura - Threshold condition for spray formation by Faraday instability(2).pdf:pdf},
issn = {14697645},
journal = {Journal of Fluid Mechanics},
keywords = {Aerosols/atomization,Faraday waves,Gas/liquid flow},
pages = {73--103},
title = {{Threshold condition for spray formation by Faraday instability}},
volume = {759},
year = {2014}
}
