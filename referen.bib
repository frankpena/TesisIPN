	File = {:Users/fpena/Documents/Mendeley Desktop/Skeldon, Rucklidge - 2015 - Can weakly nonlinear theory explain Faraday wave patterns near onset.pdf:pdf},
	Issn = {0022-1120},
	Journal = {Journal of Fluid Mechanics},
	Keywords = {faraday waves,parametric instability,pattern formation},
	Pages = {604--632},
	Title = {{Can weakly nonlinear theory explain Faraday wave patterns near onset?}},
	Url = {http://www.journals.cambridge.org/abstract{\_}S0022112015003882},
	Volume = {777},
	Year = {2015},
	Bdsk-Url-1 = {http://www.journals.cambridge.org/abstract%7B%5C_%7DS0022112015003882},
