isbn = {0167-2789},
issn = {0167-2789},
journal = {Physica D: Nonlinear Phenomena},
keywords = {Hexagons,Patterns,Superlattice,Surface waves,Symmetries},
number = {1-4},
pages = {99--111},
primaryClass = {chao-dyn},
title = {{Superlattice patterns in surface waves}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0167278998001158},
