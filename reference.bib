file = {:C$\backslash$:/Users/fpena/Documents/Mendeley Desktop/2019 - Hua et al. - Experimental and numerical study on the effect of dimensionless parameters on the characteristics of droplet atomiza.pdf:pdf},
issn = {00162361},
journal = {Fuel},
keywords = {Atomization characteristics,Critical condition,Dimensionless parameters,Fuel secondary atomization,Single droplet},
number = {May},
pages = {941--949},
publisher = {Elsevier},
title = {{Experimental and numerical study on the effect of dimensionless parameters on the characteristics of droplet atomization caused by periodic inertial force}},
url = {https://doi.org/10.1016/j.fuel.2019.05.083},
volume = {253},
year = {2019}
}
