primaryClass = {arXiv:patt-sol},
title = {{Square Patterns and Quasi-patterns in Weakly Damped Faraday Waves}},
url = {http://arxiv.org/abs/patt-sol/9603005},
volume = {53},
year = {1996}
}
@article{Zhang1997,
abstract = {We present a theoretical study of nonlinear pattern formation in parametric$\backslash$nsurface waves for fluids of low viscosity, and in the limit of large$\backslash$naspect ratio. The analysis is based on a quasi-potential approximation$\backslash$nto the equations governing fluid motion, followed by a multiscale$\backslash$nasymptotic expansion in the distance away from threshold. Close to$\backslash$nonset, the asymptotic expansion yields an amplitude equation which$\backslash$nis of gradient form, and allows the explicit calculation of the functional$\backslash$nform of the cubic nonlinearities. In particular, we find that three-wave$\backslash$nresonant interactions contribute significantly to the nonlinear terms,$\backslash$nand therefore are important for pattern selection. Minimization of$\backslash$nthe associated Lyapunov functional predicts a primary bifurcation$\backslash$nto a standing wave pattern of square symmetry for capillary-dominated$\backslash$nsurface waves, in agreement with experiments. In addition, we find$\backslash$nthat patterns of hexagonal and quasi-crystalline symmetry can be$\backslash$nstabilized in certain mixed capillary-gravity waves, even in this$\backslash$ncase of single-frequency forcing. Quasi-crystalline patterns are$\backslash$npredicted in a region of parameters readily accessible experimentally.},
