issn = {0307904X},
journal = {Applied Mathematical Modelling},
keywords = {Computational fluid dynamics,K-{\{}small element of{\}} model,Sudden-expansion flow,Turbulent flow},
number = {21},
pages = {6711--6724},
title = {{Turbulent models of oil flow in a circular pipe with sudden enlargement}},
volume = {39},
year = {2015}
}
