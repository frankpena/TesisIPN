@article{Higginbotham2011b,
	Abstract = {A popular method for generating micron-sized aerosols is to submerge ultrasonic ($\omega${\~{}}MHz) piezoelectric oscillators in a water bath. The submerged oscillator atomizes the fluid, creating droplets with radii proportional to the wavelength of the standing wave at the fluid surface. Classical theory for the Faraday instability predicts a parametric instability driving a capillary wave at the subharmonic ($\omega$/2) frequency. For many applications it is desirable to reduce the size of the droplets; however, using higher frequency oscillators becomes impractical beyond a few MHz. Observations are presented that demonstrate that smaller droplets may also be created by increasing the driving amplitude of the oscillator, and that this effect becomes more pronounced for large driving frequencies. It is shown that these observations are consistent with a transition from droplets associated with subharmonic ($\omega$/2) capillary waves to harmonic ($\omega$) capillary waves induced by larger driving frequencies and amplitudes, as predicted by a stability analysis of the capillary waves.},
	Author = {Higginbotham, a P and Guillen, A and Jones, N and Donnelly, T. D. and Bernoff, Andrew J.},
	Doi = {10.1121/1.3643816},
	File = {:Users/fpena/Documents/Mendeley Desktop/Higginbotham et al. - 2011 - Evidence of the harmonic Faraday instability in ultrasonic atomization experiments with a deep, inviscid fl.pdf:pdf},
	Issn = {1520-8524},
	Journal = {The Journal of the Acoustical Society of America},
	Keywords = {Aerosols,Models,Nebulizers and Vaporizers,Oscillometry,Particle Size,Sonication,Surface Properties,Theoretical,Ultrasonics,Ultrasonics: instrumentation},
	Month = {nov},
	Number = {5},
	Pages = {2694--9},
	Pmid = {22087897},
	Title = {{Evidence of the harmonic Faraday instability in ultrasonic atomization experiments with a deep, inviscid fluid.}},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/22087897},
	Volume = {130},
	Year = {2011},
	Bdsk-Url-1 = {http://www.ncbi.nlm.nih.gov/pubmed/22087897},
